{
  "name": "mqtt",
  "version": "1.0.0",
  "description": "device service mqtt",
  "main": "index.js",
  "bin": "./index.js",
  "scripts": {
    "start": "nodemon --inspect index.js",
    "setup_armhf": "export AR=\"arm-linux-gnueabihf-ar\" && export CC=\"arm-linux-gnueabihf-gcc\" && export CXX=\"arm-linux-gnueabihf-g++\" &&export LINK=\"arm-linux-gnueabihf-g++\"&&npm install --build-from-source --target_arch=arm --omit=dev",
    "setup": "npm install --omit=dev",
    "build": "rm -f -r ./dist && DOCKER_BUILDKIT=1 docker build --output dist . --no-cache=true",
    "clean": "rm -f -r ./dist & rm -f -r ./node_modules & rm -f -r ./store"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Noobmaster69-bug/ds-mqtt.git"
  },
  "keywords": [
    "IoT",
    "Edge",
    "device",
    "service"
  ],
  "author": "Uy Son Tran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Noobmaster69-bug/ds-mqtt/issues"
  },
  "homepage": "https://github.com/Noobmaster69-bug/ds-mqtt#readme",
  "dependencies": {
    "commist": "^1.0.0",
    "concat-stream": "^2.0.0",
    "debug": "^4.1.1",
    "duplexify": "^4.1.1",
    "express": "^4.18.1",
    "help-me": "^3.0.0",
    "inherits": "^2.0.3",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "minimist": "^1.2.5",
    "morgan": "^1.10.0",
    "mqtt-nedb-store": "^0.1.1",
    "mqtt-packet": "^6.8.0",
    "mqtt-wildcard": "^3.0.9",
    "number-allocator": "^1.0.9",
    "pump": "^3.0.0",
    "readable-stream": "^3.6.0",
    "reinterval": "^1.1.0",
    "rfdc": "^1.3.0",
    "split2": "^3.1.0",
    "ws": "^7.5.5",
    "xtend": "^4.0.2"
  },
  "pkg": {
    "script": "*.js",
    "targets": [
      "node16-linux-armv7"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  }
}
